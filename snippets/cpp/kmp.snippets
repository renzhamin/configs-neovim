	void kmp(string &text,string &pattern){
		int n=pattern.length(),l=text.length(),lps[n],i,j,match=0;
		lps[0]=0;
		//j-----i//
		for(i=1,j=0;i<n;){
			if(pattern[j]==pattern[i]) lps[i]=j+1,i++,j++;
			else {
				if(j) j=lps[j-1];
				else lps[i]=0,i++;
			}
		}
		//j---------i//
		i=0,j=0;
		while(i<l){
			if(pattern[j]==text[i])
				i++,j++;
			else {
				if(j) j=lps[j-1];
				else i++;
			}
			if(j==n){
				j=lps[j-1];
				match++;
			}
		}
	}


